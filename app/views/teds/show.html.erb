<div class="container">
  <div class="row">
    <p> <%= t('.name') %> <%= @ted.name %></p>

    <% @ted.chapters.each do |chapter|  %>
      <div class="chapter">
        <title>Going through Birmania</title>
        <div class="chapter-illustrations">
          <% chapter.photos.each do |photo| %>
            <%= cl_image_tag(photo.image.path, { size: '125x125', crop: :fit }) %>
          <% end %>
        </div>
      </div>
    <% end %>

  </div>
</div>
